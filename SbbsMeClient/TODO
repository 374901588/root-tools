3. 相册
4. 数据统计

http://sbbs.me/uploads/1756787011/51f1ddae60e7940d07000001.jpg

create table sbbs_log(
	id int primary key auto_increment not null, 
	device text not null, 
	appver int not null, 
	osver text not null,
	email text, 
	action text not null, 
	detail text, 
	actiontime text not null
) character set utf8;

create table sbbs_feedback(
	id int primary key auto_increment, 
	device text, 
	appver int not null, 
	osver text not null, 
	email text, 
	userid text, 
	feedback text
) character set utf8;

select count(*) c from (select distinct device,osver,email from sbbs_log) a